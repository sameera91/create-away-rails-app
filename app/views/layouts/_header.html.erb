<br>
<div id="title">
<%= image_tag("Drawing.png") %><br><br>
<% if current_user %>
   <div class="welcome">Welcome <%= current_user.name %>!<br></div>
<% end %>
</div>
<br><br>
<nav class="nav">
    <ul>
          <% if current_user %>
            <%= link_to "Sign out", destroy_user_session_path, :class => "nav_button", method: :delete %>
          <%else%>
            <%= link_to "Sign In", user_session_path, :class => "nav_button"%>
            <%= link_to "Sign up", new_user_registration_path, :class => "nav_button" %>
          <% end %> 

          <%if current_user%>
            <%= link_to "New project", new_user_project_path(current_user.id), :class => "nav_button" %>
            <%= link_to "My projects", user_projects_path(current_user.id), :class => "nav_button" %>
          <%end%>

          <%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook), :class => "nav_button" %>

          <%= link_to "All projects", projects_path, :class => "nav_button" %>
    </ul>
</nav>